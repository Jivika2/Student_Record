<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template id="student_id_card_template">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <div class="page">
                    <h2>Student ID Card</h2>
                    <p><strong>Student Name:</strong> <t t-esc="doc.name"/></p>
                    <p><strong>Birthdate:</strong> <t t-esc="doc.birthdate"/></p>
                    <p><strong>Age:</strong> <t t-esc="doc.age"/></p>
                </div>
            </t>
        </t>
    </template>
</odoo>
